            <tab FONT="FreeMono Bold"/>
            <tab FONT="FreeMono Bold"/>
            <tab FONT="FreeMono Bold"/>
            <ITEXT FONT="FreeMono Bold" CH="lasers[len(lasers)-1].status = 0"/>
            <para/>
