            <para/>
            <ITEXT FONT="FreeMono Regular" CH="            aliens[a].image = &quot;alien1b&quot;"/>
            <para/>
            <tab/>
            <tab/>
            <ITEXT CH="    "/>
            <ITEXT FONT="FreeMono Bold" CH="if randint(0,5) == 0:"/>
            <para/>
            <tab FONT="FreeMono Bold"/>
            <tab FONT="FreeMono Bold"/>
            <tab FONT="FreeMono Bold"/>
            <tab FONT="FreeMono Bold"/>
